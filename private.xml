<?xml version="1.0"?>
<root>
<list>
    <item>
        <name>Airy Configuration</name>
        <item>
            <name>Config</name>
            <item>
                <name>Terminal And Vim Setting</name>
                <identifier>private.app_in_terminal_settings</identifier>
                <only>TERMINAL</only>
                <autogen>__KeyOverlaidModifier__ KeyCode::JIS_KANA, KeyCode::VK_MODIFIER_EXTRA1, KeyCode::JIS_KANA</autogen>
                <autogen>__KeyOverlaidModifier__ KeyCode::JIS_EISUU, KeyCode::VK_MODIFIER_EXTRA2, KeyCode::JIS_EISUU</autogen>
                <autogen>__KeyToKey__ <!-- Bracket Snippet -->
                    KeyCode::SEMICOLON, ModifierFlag::EXTRA1 | ModifierFlag::EXTRA2,
                    KeyCode::JIS_BRACKET_LEFT, ModifierFlag::SHIFT_L, KeyCode::JIS_BRACKET_RIGHT, ModifierFlag::SHIFT_L, KeyCode::CURSOR_LEFT, KeyCode::ENTER, KeyCode::ENTER, KeyCode::CURSOR_UP, KeyCode::TAB
                </autogen>
                <autogen>__KeyToKey__ <!-- In InsertMode: delete word block, In NormalMode: split window move modifier -->
                    KeyCode::R, ModifierFlag::EXTRA1,
                    KeyCode::W, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- Redo -->
                    KeyCode::H, ModifierFlag::EXTRA1,
                    KeyCode::R, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- Scroll Up Editor Window -->
                    KeyCode::G, ModifierFlag::EXTRA1,
                    KeyCode::B, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- Scroll Down Editor Window -->
                    KeyCode::V, ModifierFlag::EXTRA1,
                    KeyCode::F, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- In Terminal: exit, In Vim: mini scroll down -->
                    KeyCode::C, ModifierFlag::EXTRA1,
                    KeyCode::D, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- Quit -->
                    KeyCode::C, ModifierFlag::EXTRA2,
                    KeyCode::C, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- Enter Visual Block Mode -->
                    KeyCode::B, ModifierFlag::EXTRA2,
                    KeyCode::V, ModifierFlag::CONTROL_L
                </autogen>
                <autogen>__KeyToKey__ <!-- For Unite Prefix -->
                    KeyCode::U, ModifierFlag::EXTRA1,
                    KeyCode::U, ModifierFlag::CONTROL_L
                </autogen>
            </item>
        </item>
        <item>
            <name>Zenisyns</name>
            <item>
                <name>SX_XR1</name>
                <identifier>private.xr1</identifier>
                <autogen>__DoublePressModifier__
                    KeyCode::COMMAND_R, KeyCode::COMMAND_R,
                    KeyCode::ESCAPE, KeyCode::JIS_EISUU
                </autogen>
                <autogen>__KeyOverlaidModifier__ KeyCode::JIS_KANA, KeyCode::VK_MODIFIER_EXTRA1, KeyCode::JIS_KANA</autogen>
                <autogen>__KeyToKey__ <!-- I -> PAGETOP -->
                    KeyCode::I, ModifierFlag::EXTRA1,
                    KeyCode::HOME
                </autogen>
                <autogen>__KeyToKey__ <!-- ; -> ENTER -->
                    KeyCode::SEMICOLON, ModifierFlag::EXTRA1,
                    KeyCode::RETURN
                </autogen>
                <autogen>__KeyToKey__ <!-- SPACE -> BACKSPACE -->
                    KeyCode::SPACE, ModifierFlag::EXTRA1,
                    KeyCode::DELETE
                </autogen>
                <autogen>__KeyToKey__ <!-- : -> TAB -->
                    KeyCode::JIS_COLON, ModifierFlag::EXTRA1,
                    KeyCode::TAB
                </autogen>
                <!-- CURSOL MOVE -->
                <autogen>__KeyToKey__ KeyCode::S, ModifierFlag::EXTRA1, KeyCode::CURSOR_LEFT</autogen>
                <autogen>__KeyToKey__ KeyCode::F, ModifierFlag::EXTRA1, KeyCode::CURSOR_RIGHT</autogen>
                <autogen>__KeyToKey__ KeyCode::E, ModifierFlag::EXTRA1, KeyCode::CURSOR_UP</autogen>
                <autogen>__KeyToKey__ KeyCode::D, ModifierFlag::EXTRA1, KeyCode::CURSOR_DOWN</autogen>
                <autogen>__KeyToKey__ <!--  J -> : -->
                    KeyCode::J, ModifierFlag::EXTRA1,
                    KeyCode::JIS_COLON
                </autogen>
                <autogen>__KeyToKey__ <!-- K -> - -->
                    KeyCode::K, ModifierFlag::EXTRA1,
                    KeyCode::MINUS
                </autogen>
                <autogen>__KeyToKey__ <!-- A -> & -->
                    KeyCode::A, ModifierFlag::EXTRA1,
                    KeyCode::KEY_6, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- P -> \ -->
                    KeyCode::P, ModifierFlag::EXTRA1,
                    KeyCode::JIS_YEN
                </autogen>
                <autogen>__KeyToKey__ <!-- L -> _ -->
                    KeyCode::L, ModifierFlag::EXTRA1,
                    KeyCode::JIS_UNDERSCORE
                </autogen>
                <autogen>__KeyToKey__ <!-- M -> $ -->
                    KeyCode::M, ModifierFlag::EXTRA1,
                    KeyCode::KEY_4, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- N -> ^ -->
                    KeyCode::N, ModifierFlag::EXTRA1,
                    KeyCode::JIS_HAT
                </autogen>
                <autogen>__KeyToKey__ <!-- B -> % -->
                    KeyCode::B, ModifierFlag::EXTRA1,
                    KeyCode::KEY_5, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- O -> @ -->
                    KeyCode::O, ModifierFlag::EXTRA1,
                    KeyCode::JIS_ATMARK
                </autogen>
            </item>
            <item>
                <name>SX_XL1</name>
                <identifier>private.xl1</identifier>
                <autogen>--KeyOverlaidModifier-- KeyCode::JIS_EISUU, KeyCode::VK_MODIFIER_EXTRA2, KeyCode::JIS_EISUU</autogen>
                <autogen>__KeyToKey__ <!-- SPACE -> DELETE -->
                    KeyCode::SPACE, ModifierFlag::EXTRA2,
                    KeyCode::FORWARD_DELETE
                </autogen>
                <autogen>__KeyToKey__ <!-- V -> SCROLLUP -->
                    KeyCode::V, ModifierFlag::EXTRA2,
                    KeyCode::VK_MOUSEKEY_SCROLL_UP
                </autogen>
                <autogen>__KeyToKey__ <!-- G -> SCROLLDOWN -->
                    KeyCode::G, ModifierFlag::EXTRA2,
                    KeyCode::VK_MOUSEKEY_SCROLL_DOWN
                </autogen>
                <!-- {}[] -->
                <autogen>__KeyToKey__ KeyCode::K, ModifierFlag::EXTRA2, KeyCode::JIS_BRACKET_LEFT</autogen>
                <autogen>__KeyToKey__ KeyCode::L, ModifierFlag::EXTRA2, KeyCode::JIS_BRACKET_RIGHT</autogen>
                <!-- ()<> -->
                <autogen>__KeyToKey__ KeyCode::J, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L, KeyCode::COMMA, ModifierFlag::SHIFT_L</autogen>
                <autogen>__KeyToKey__ KeyCode::I, ModifierFlag::EXTRA2 | ModifierFlag::SHIFT_L, KeyCode::DOT, ModifierFlag::SHIFT_L</autogen>
                <autogen>__KeyToKey__ KeyCode::J, ModifierFlag::EXTRA2, KeyCode::KEY_8, ModifierFlag::SHIFT_L</autogen>
                <autogen>__KeyToKey__ KeyCode::I, ModifierFlag::EXTRA2, KeyCode::KEY_9, ModifierFlag::SHIFT_L</autogen>
                <autogen>__KeyToKey__ <!-- M -> + -->
                    KeyCode::M, ModifierFlag::EXTRA2,
                    KeyCode::SEMICOLON, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- P -> * -->
                    KeyCode::P, ModifierFlag::EXTRA2,
                    KeyCode::JIS_COLON, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- O -> ! -->
                    KeyCode::O, ModifierFlag::EXTRA2,
                    KeyCode::KEY_1, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- ; -> = -->
                    KeyCode::SEMICOLON, ModifierFlag::EXTRA2,
                    KeyCode::MINUS, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- H -> " -->
                    KeyCode::H, ModifierFlag::EXTRA2,
                    KeyCode::KEY_2, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- N -> ' -->
                    KeyCode::N, ModifierFlag::EXTRA2,
                    KeyCode::KEY_7, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- A -> , -->
                    KeyCode::A, ModifierFlag::EXTRA2,
                    KeyCode::COMMA
                </autogen>
                <autogen>__KeyToKey__ <!-- S -> # -->
                    KeyCode::S, ModifierFlag::EXTRA2,
                    KeyCode::KEY_3, ModifierFlag::SHIFT_L
                </autogen>
                <autogen>__KeyToKey__ <!-- D -> . -->
                    KeyCode::D, ModifierFlag::EXTRA2,
                    KeyCode::DOT
                </autogen>
                <autogen>__KeyToKey__ <!-- F -> / -->
                    KeyCode::F, ModifierFlag::EXTRA2,
                    KeyCode::SLASH
                </autogen>
                <autogen>__KeyToKey__ <!-- Emmet Plugin Execute  -->
                    KeyCode::E, ModifierFlag::EXTRA2,
                    KeyCode::E, ModifierFlag::CONTROL_L
                </autogen>
                <!-- Function Keys -->
                <autogen>__KeyToKey__ KeyCode::KEY_1, ModifierFlag::EXTRA2, KeyCode::F1</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_2, ModifierFlag::EXTRA2, KeyCode::F2</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_3, ModifierFlag::EXTRA2, KeyCode::F3</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_4, ModifierFlag::EXTRA2, KeyCode::F4</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_5, ModifierFlag::EXTRA2, KeyCode::F5</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_6, ModifierFlag::EXTRA2, KeyCode::F6</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_7, ModifierFlag::EXTRA2, KeyCode::F7</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_8, ModifierFlag::EXTRA2, KeyCode::F8</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_9, ModifierFlag::EXTRA2, KeyCode::F9</autogen>
                <autogen>__KeyToKey__ KeyCode::KEY_0, ModifierFlag::EXTRA2, KeyCode::F10</autogen>
                <autogen>__KeyToKey__ KeyCode::MINUS, ModifierFlag::EXTRA2, KeyCode::F11</autogen>
                <autogen>__KeyToKey__ KeyCode::JIS_HAT, ModifierFlag::EXTRA2, KeyCode::F12</autogen>
            </item>
            <item>
                <name>HHKb</name>
                <identifier>private.hhkb</identifier>
                <!-- Replace each other Ctr & Cmd -->
                <autogen>__KeyToKey__ KeyCode::CONTROL_L, KeyCode::COMMAND_L</autogen>
                <autogen>__KeyToKey__ KeyCode::COMMAND_L, KeyCode::CONTROL_L</autogen>
            </item>
