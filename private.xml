        <item>
            <name>Mouse Keys</name>
            <item>
                <name>Simultaneous Key Presses XL1 + XR1</name>
                <identifier vk_config="true">notsave.private.mousekeys_mode</identifier>
                <!-- Scroll -->
                <autogen>__KeyToKey__ KeyCode::F, ModifierFlag::FN, KeyCode::VK_MOUSEKEY_SCROLL_LEFT,  ModifierFlag::FN</autogen>
                <autogen>__KeyToKey__ KeyCode::E, ModifierFlag::FN, KeyCode::VK_MOUSEKEY_SCROLL_DOWN,  ModifierFlag::FN</autogen>
                <autogen>__KeyToKey__ KeyCode::D, ModifierFlag::FN, KeyCode::VK_MOUSEKEY_SCROLL_UP,    ModifierFlag::FN</autogen>
                <autogen>__KeyToKey__ KeyCode::S, ModifierFlag::FN, KeyCode::VK_MOUSEKEY_SCROLL_RIGHT, ModifierFlag::FN</autogen>
                <!-- Mouse Pointer Motion -->
                <autogen>__KeyToKey__ KeyCode::S, KeyCode::VK_MOUSEKEY_LEFT</autogen>
                <autogen>__KeyToKey__ KeyCode::D, KeyCode::VK_MOUSEKEY_DOWN</autogen>
                <autogen>__KeyToKey__ KeyCode::E, KeyCode::VK_MOUSEKEY_UP</autogen>
                <autogen>__KeyToKey__ KeyCode::F, KeyCode::VK_MOUSEKEY_RIGHT</autogen>
                <!-- Mouse Click -->
                <block>
                    <config_only>private.mousekeys_mode_commandL2leftclick</config_only>
                    <autogen>__KeyToPointingButton__ KeyCode::A, PointingButton::LEFT</autogen>
                </block>
                <block>
                    <config_only>private.mousekeys_mode_space2middleclick</config_only>
                    <autogen>__KeyToPointingButton__ KeyCode::R, PointingButton::MIDDLE</autogen>
                </block>
                <block>
                    <config_only>private.mousekeys_mode_commandR2rightclick</config_only>
                    <autogen>__KeyToPointingButton__ KeyCode::W, PointingButton::RIGHT</autogen>
                </block>
                <!-- Scroll & Mouse Motion Option -->
                <block>
                    <config_only>private.simultaneouskeypresses_mouse_keys_mode_ds_strict_key_order</config_only>
                    <!-- Scroll Modifier to K -->
                    <autogen>__KeyToKey__ KeyCode::K, KeyCode::FN</autogen>
                    <!-- High Speed Mouse Move -->
                    <autogen>__KeyToKey__ KeyCode::J, KeyCode::VK_MOUSEKEY_HIGHSPEED</autogen>
                </block>
            </item>
            <item>
                <name>Pointer Move</name>
                <identifier>private.simultaneouskeypresses_mouse_keys_mode_ds_strict_key_order</identifier>
                <!-- Commit Mouse Mode -->
                <autogen>__SimultaneousKeyPresses__ KeyCode::JIS_EISUU, KeyCode::F, KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_private_mousekeys_mode</autogen>
            </item>
            <item>
                <name>Left Click</name>
                <identifier>private.mousekeys_mode_commandL2leftclick</identifier>
            </item>
            <item>
                <name>Middle Click</name>
                <identifier>private.mousekeys_mode_space2middleclick</identifier>
            </item>
            <item>
                <name>Right Click</name>
                <identifier>private.mousekeys_mode_commandR2rightclick</identifier>
            </item>
        </item>
    </item>
